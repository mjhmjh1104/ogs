<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Redirections</title>
</head>
<body>
    <h1>Redirections</h1>
    <table>
        <tr>
            <td>#</td>
            <td>To</td>
            <td>Functions</td>
        </tr>
<% redirections.forEach(function (item) { %>
        <tr>
            <td><%= item.num %></td>
            <td><a href="/view/<%= item.href %>"><%= item.href %></a></td>
            <td><% if (user && user.admin) { %><form method="POST" action="/redirections/<%= item.num %>/delete" accept-charste="utf-8">
                <button type="submit">Remove</button>
            </form><% } %>
            <a href="/history/<%= item.num %>"><button>History</button></a></td>
        </tr>
<% }); %>
    </table>
    <a href="/">Main</a>
</body>
</html>